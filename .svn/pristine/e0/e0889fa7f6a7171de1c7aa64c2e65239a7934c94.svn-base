/*Tabela*/

/*tabela*/
.table thead tr th,
.sticky-thead thead tr th {
    border-bottom: 2px solid #163d6d;
    text-align: left;
    background: #3d4d5a;  /*#6799af;*/
    border-bottom: none;
    color: #fff;
    font-weight: lighter;
    font-family: 'Roboto', sans-serif;
    font-size:1em;
    padding-left:15px;
}

.table-striped > tbody > tr:nth-child(odd) {
   background-color: #e9e6e6;
}

.table tbody tr td {
    cursor: pointer;
    font-family: 'Roboto', sans-serif;
    font-size:1em;
    padding-left:15px;
}

.table-striped > tbody > tr:hover {
    background-color: #546877;
    color:#fff;
}



