/*Botões*/

.buttonInside {
    cursor: pointer;
    margin-top: 15px;
    padding: 10px 20px;
    border: none;
    color: #fff;
    background:#991426;
}
.buttonInside:hover {
    background-color: #777c81;
}

.buttonFooter {
    cursor: pointer;
    padding: 10px 20px;
    border: none;
    color: #fff;
    background:#991426;
    margin-left:2px;
}

.buttonFooter:hover {
    background-color: #777c81;
}

.buttonFooter:disabled{
    background-color: #D7D7D7;
}


button.close {
    color:#fff;
    opacity:1;
}

.buttonPagamento {
    cursor: pointer;
    padding: 5px 10px;
    border: none;
    color: #fff;
    background:#3d4d5a;
    margin-top: 10px;
    margin-left:2px;
}

.buttonPagamento:hover {
    background-color: #777c81;
}


